{
  "expo": {
    "name": "UPagos",
    "slug": "UPagos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#363062"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#363062"
      },
      "package": "com.karimali.UPagos",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9f334026-4f94-41e3-8dbd-9d28e8ad9901"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/9f334026-4f94-41e3-8dbd-9d28e8ad9901"
    },
    "ios": {
      "runtimeVersion": {
        "policy": "appVersion"
      }
    }
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "apk"
      }
    }
  }
}
